<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd" version="1.2">
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>index</from-outcome>
			<to-view-id>/index/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-action>#{fooMB.list}</from-action>
			<to-view-id>/test/foo-list.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/test/foo-list.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{fooMB.delete}</from-action>
			<to-view-id>/test/foo-list.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{fooMB.edit}</from-action>
			<to-view-id>/test/foo-save.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>save</from-outcome>
			<to-view-id>/test/foo-save.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/test/foo-save.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{fooMB.save}</from-action>
			<from-outcome>success</from-outcome>
			<to-view-id>/test/foo-save.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-action>#{fooMB.update}</from-action>
			<from-outcome>success</from-outcome>
			<to-view-id>/test/foo-save.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
